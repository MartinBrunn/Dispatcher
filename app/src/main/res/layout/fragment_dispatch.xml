<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".DispatcherActivityFragment">

    <TextView android:id="@+id/textview"
        android:text="@string/hello_world" android:layout_width="match_parent"
        android:textColor="@color/red"
        android:background="@color/yellow"
        android:layout_height="@dimen/BigText" android:textAppearance="@style/AppFonts"/>

    <ImageView android:id="@+id/divider"
        android:text="@string/hello_world" android:layout_width="match_parent"
        android:src="@drawable/ic_drawer"
        android:background="@color/yellow"
        android:layout_height="@dimen/BigText"/>
    <!-- WebView android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview">
    </WebView -->
    <ListView android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:entries="@array/listentries"
        android:layout_below="@+id/textview"
        android:textAppearance="@style/AppFonts">
    </ListView>
</RelativeLayout>
